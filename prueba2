CREATE TABLE CLIENTES
(
ID INT auto_increment PRIMARY KEY,
DNI INT UNSIGNED NOT NULL UNIQUE KEY,
NOMBRE VARCHAR(30) NOT NULL,
APELLIDO VARCHAR(30) NULL,
SUELDO FLOAT NOT NULL,
FEC_INI DATE NOT NULL,
FEC_VEC DATE NOT NULL
);
INSERT INTO CLIENTES(DNI, NOMBRE, APELLIDO, SUELDO, FEC_INI, FEC_VEC)
VALUES              (397654, 'JUAN', 'GOMEZ', 182600, '2022-11-18', '2024-01-01'),
                    (975463, 'MARIA', 'PEREZ', 141500, '2020-11-26', '2023-09-20'),
                    (557136, 'PEDRO', 'VIZUTI', 115300, '2021-06-12', '2022-02-02');
SELECT * FROM CLIENTES;
SELECT * FROM CLIENTES WHERE NOMBRE = 'MARIA' AND APELLIDO = 'PEREZ';
SELECT * FROM CLIENTES WHERE SUELDO >= 141500 AND SUELDO <= 182600;
SELECT * FROM CLIENTES WHERE SUELDO BETWEEN 141500 AND 182600;
SELECT * FROM CLIENTES WHERE SUELDO NOT BETWEEN 141500 AND 182600;

SELECT * FROM CLIENTES;
SELECT * FROM CLIENTES WHERE NOMBRE = 'MARIA' OR NOMBRE = 'PEPE';
SELECT * FROM CLIENTES WHERE NOMBRE IN('MARIA','PEPE');
SELECT * FROM CLIENTES WHERE NOMBRE NOT IN('MARIA','PEPE');
SELECT * FROM CLIENTES WHERE NOMBRE LIKE 'M%';
SELECT * FROM CLIENTES WHERE NOMBRE LIKE '%A';
SELECT * FROM CLIENTES WHERE NOMBRE LIKE '%A%';
SELECT * FROM CLIENTES WHERE SUELDO LIKE '%1%' ORDER BY SUELDO LIMIT 2;
INSERT INTO CLIENTES(DNI, NOMBRE, APELLIDO, SUELDO, FEC_INI, FEC_VEC)
VALUES              (887459, 'JOAN', NULL, 194200, '2021-10-4', '2024-02-01');
SELECT * FROM CLIENTES;
SELECT * FROM CLIENTES WHERE APELLIDO = NULL; -- ESTO NO SIRVE
SELECT * FROM CLIENTES WHERE APELLIDO = 'NULL'; -- ESTO NO SIRVE
SELECT * FROM CLIENTES WHERE APELLIDO IS NULL; -- ESTO SI SIRVE
SELECT * FROM CLIENTES WHERE APELLIDO IS NOT NULL; -- ESTO NO SIRVE

-- ALIAS O LITERALES
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS SUELDO_NUEVO FROM CLIENTES;
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS SUELDO_NUEVO FROM CLIENTES WHERE SUELDO*1.15 >= 200000; -- FUNCIONA
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS SUELDO_NUEVO FROM CLIENTES WHERE SUELDO_NUEVO >= 200000;  -- NO FUNCIONA
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS SUELDO_NUEVO FROM CLIENTES HAVING SUELDO_NUEVO >= 200000; -- FUNCIONA
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS SUELDO_NUEVO FROM CLIENTES HAVING SUELDO*1.15 >= 200000; -- FUNCIONA
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS 'ESTE ES EL SUELDO NUEVO DE LOS EMPLEADOS' FROM CLIENTES;
SELECT NOMBRE, APELLIDO, SUELDO, SUELDO*1.15 AS SUELDO_NUEVO FROM CLIENTES;
DESC CLIENTES; 
SELECT * FROM CLIENTES;
SELECT NOMBRE, APELLIDO, YEAR(FEC_INI) AS 'AÑO DE INICIO' FROM CLIENTES WHERE NOMBRE = 'MARIA';
SELECT NOMBRE, APELLIDO, YEAR(FEC_VEC) AS 'AÑO DE VENCIMIENTO' FROM CLIENTES WHERE NOMBRE = 'MARIA';
SELECT NOMBRE, APELLIDO, MONTH(FEC_INI) AS 'MES DE INICIO' FROM CLIENTES WHERE NOMBRE = 'MARIA';
SELECT NOMBRE, APELLIDO, MONTH(FEC_VEC) AS 'MES DE VENCIMIENTO' FROM CLIENTES ;
SELECT NOMBRE, APELLIDO, DAY(FEC_INI) AS 'MES DE INICIO' FROM CLIENTES;
SELECT NOMBRE, APELLIDO, DAY(FEC_INI) AS 'DIA DE INICIO', MONTH(FEC_INI) AS 'MES DE INICIO', 
YEAR(FEC_INI) AS 'AÑO DE INICIO' FROM CLIENTES;
SELECT current_date();
SELECT curdate();
SELECT YEAR(current_date());
SELECT month(curdate());
SELECT dayofyear(CURDATE());
SELECT current_time();
SELECT curtime();
